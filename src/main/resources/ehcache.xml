<?xml version="1.0" encoding="UTF-8"?>


<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false"
	monitoring="autodetect" dynamicConfig="true">

	<cache name="addResultCache" maxElementsInMemory="10" eternal="false"
		overflowToDisk="false" timeToIdleSeconds="60" timeToLiveSeconds="120"
		memoryStoreEvictionPolicy="LFU" />

	<cache name="subtractResultCache" maxElementsInMemory="10"
		eternal="false" overflowToDisk="false" timeToIdleSeconds="60"
		timeToLiveSeconds="120" memoryStoreEvictionPolicy="LFU" />

	<cache name="multiplyResultCache" maxElementsInMemory="10"
		eternal="false" overflowToDisk="false" timeToIdleSeconds="60"
		timeToLiveSeconds="120" memoryStoreEvictionPolicy="LFU" />

	<cache name="divideResultCache" maxElementsInMemory="10" eternal="false"
		overflowToDisk="false" timeToIdleSeconds="60" timeToLiveSeconds="120"
		memoryStoreEvictionPolicy="LFU" />

</ehcache>